# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual API keys and configuration

# API Base URL
REACT_APP_API_BASE_URL=http://localhost:5000/api

# Socket.IO Configuration
REACT_APP_SOCKET_URL=http://localhost:5000

# Cloudinary Configuration (for image uploads and avatar management)
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
REACT_APP_CLOUDINARY_API_KEY=your_cloudinary_api_key
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Google Maps API (for advanced mapping features)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Weather API Configuration
REACT_APP_WEATHER_API_KEY=your_openweathermap_api_key

# Unsplash API (for destination photos)
REACT_APP_UNSPLASH_ACCESS_KEY=your_unsplash_access_key

# Travel Data APIs
REACT_APP_AMADEUS_API_KEY=your_amadeus_api_key
REACT_APP_AMADEUS_API_SECRET=your_amadeus_api_secret

# Analytics and Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_tracking_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_COLLABORATION=true
REACT_APP_ENABLE_AI_SUGGESTIONS=true
REACT_APP_ENABLE_FLIGHT_TRACKING=true
REACT_APP_ENABLE_EXPENSE_TRACKING=true

# Social Authentication
REACT_APP_GOOGLE_CLIENT_ID=your_google_oauth_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Payment Integration (for premium features)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Third-party Services
REACT_APP_MAPBOX_ACCESS_TOKEN=your_mapbox_token
REACT_APP_FOURSQUARE_API_KEY=your_foursquare_api_key

# Development/Production Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true
REACT_APP_ENABLE_DEVTOOLS=true

# CDN Configuration
REACT_APP_CDN_URL=https://your-cdn-domain.com

# App Configuration
REACT_APP_APP_NAME=Wanderlog Clone
REACT_APP_APP_VERSION=1.0.0
REACT_APP_SUPPORT_EMAIL=support@your-domain.com
